<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="446.0" prefWidth="488.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.AddPartController">
   <children>
      <Label layoutX="14.0" layoutY="24.0" text="Add Part">
         <font>
            <Font name="Calibri" size="24.0" />
         </font>
      </Label>
      <RadioButton fx:id="radBtnInHouse" layoutX="164.0" layoutY="28.0" mnemonicParsing="false" onMouseReleased="#togglePartSource" prefHeight="21.0" prefWidth="100.0" selected="true" text="In-House">
         <toggleGroup>
            <ToggleGroup fx:id="togglePartSource" />
         </toggleGroup>
      </RadioButton>
      <RadioButton fx:id="radBtnOursourced" layoutX="275.0" layoutY="28.0" mnemonicParsing="false" onMouseReleased="#togglePartSource" prefHeight="21.0" prefWidth="150.0" text="Outsourced" toggleGroup="$togglePartSource" />
      <Label layoutX="57.0" layoutY="103.0" text="ID" />
      <TextField fx:id="txtID" disable="true" editable="false" layoutX="196.0" layoutY="93.0" prefHeight="31.0" prefWidth="161.0" text="Auto-Gen - Disabled">
         <font>
            <Font name="System Italic" size="15.0" />
         </font>
      </TextField>
      <Label layoutX="57.0" layoutY="145.0" text="Name" />
      <TextField fx:id="txtName" layoutX="196.0" layoutY="135.0" prefHeight="31.0" prefWidth="161.0" promptText="Part Name">
         <font>
            <Font name="System Italic" size="15.0" />
         </font>
      </TextField>
      <Label layoutX="57.0" layoutY="189.0" text="Inv" />
      <TextField fx:id="txtInv" layoutX="196.0" layoutY="179.0" prefHeight="31.0" prefWidth="161.0" promptText="Inventory">
         <font>
            <Font name="System Italic" size="15.0" />
         </font>
      </TextField>
      <Label layoutX="56.0" layoutY="231.0" text="Price" />
      <TextField fx:id="txtPrice" layoutX="196.0" layoutY="221.0" prefHeight="31.0" prefWidth="161.0" promptText="Price/Cost">
         <font>
            <Font name="System Italic" size="15.0" />
         </font>
      </TextField>
      <Label layoutX="56.0" layoutY="272.0" text="Max" />
      <TextField fx:id="txtMax" layoutX="196.0" layoutY="262.0" prefHeight="31.0" prefWidth="85.0" promptText="Max">
         <font>
            <Font name="System Italic" size="15.0" />
         </font>
      </TextField>
      <Label layoutX="331.0" layoutY="267.0" text="Min" />
      <TextField fx:id="txtMin" layoutX="382.0" layoutY="262.0" prefHeight="31.0" prefWidth="85.0" promptText="Min">
         <font>
            <Font name="System Italic" size="15.0" />
         </font>
      </TextField>
      <Button fx:id="btnCancel" layoutX="402.0" layoutY="400.0" mnemonicParsing="false" onAction="#btnActionCancel" text="Cancel" />
      <Button fx:id="btnSave" layoutX="331.0" layoutY="400.0" mnemonicParsing="false" onAction="#btnActionSave" text="Save" />
      <FlowPane layoutX="55.0" layoutY="302.0" prefHeight="93.0" prefWidth="310.0">
         <children>
            <Label fx:id="labelMachineId" maxHeight="21.0" maxWidth="76.0" minHeight="0.0" minWidth="0.0" text="Machine ID" />
            <TextField fx:id="txtMachineID" maxHeight="31.0" maxWidth="161.0" minHeight="0.0" minWidth="0.0" promptText="Mach ID">
               <FlowPane.margin>
                  <Insets left="67.0" />
               </FlowPane.margin>
               <font>
                  <Font name="System Italic" size="15.0" />
               </font>
            </TextField>
            <Label fx:id="labelCompanyName" maxHeight="21.0" maxWidth="107.0" minHeight="0.0" minWidth="0.0" text="Company Name">
               <FlowPane.margin>
                  <Insets />
               </FlowPane.margin></Label>
            <TextField fx:id="txtCompanyName" maxHeight="31.0" maxWidth="161.0" minHeight="0.0" minWidth="0.0" promptText="Company Nm">
               <FlowPane.margin>
                  <Insets left="36.0" />
               </FlowPane.margin>
               <font>
                  <Font name="System Italic" size="15.0" />
               </font>
            </TextField>
         </children>
      </FlowPane>
   </children>
</AnchorPane>
